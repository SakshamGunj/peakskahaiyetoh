{
  "version": 2,
  "public": true,
  "buildCommand": null,
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/menu.html",
      "dest": "/menu.html"
    },
    {
      "src": "/styles\\.css",
      "dest": "/styles.css"
    },
    {
      "src": "/app\\.js",
      "dest": "/app.js"
    },
    {
      "src": "/(.*\\.(jpg|jpeg|png|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/peakskitchen",
      "dest": "/index.html"
    },
    {
      "src": "/awesome-burgers",
      "dest": "/index.html"
    },
    {
      "src": "/pizza-palace",
      "dest": "/index.html"
    },
    {
      "src": "/sushi-heaven",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
